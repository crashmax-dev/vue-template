{
  "name": "@vue-workspace/ui",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": "./src/index.ts",
    "./assets/*": "./src/assets/*"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "dev": "histoire dev",
    "build": "pnpm type-check && histoire build",
    "preview": "histoire preview",
    "test": "vitest run",
    "test:unit": "vitest",
    "test:unit-ui": "vitest --ui",
    "type-check": "vue-tsc --build"
  },
  "dependencies": {
    "@vueuse/core": "14.1.0",
    "class-variance-authority": "0.7.1",
    "reka-ui": "2.6.0",
    "vue": "3.5.25"
  },
  "devDependencies": {
    "@histoire/plugin-vue": "1.0.0-beta.1",
    "@vue-workspace/vite-config": "workspace:*",
    "histoire": "1.0.0-beta.1"
  }
}
