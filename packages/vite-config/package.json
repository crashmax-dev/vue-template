{
  "name": "@vue-workspace/vite-config",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./src/global.d.ts"
    }
  },
  "typesVersions": {
